{
  "name": "server",
  "version": "1.0.11",
  "private": true,
  "description": "dronies.watch backend",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist/index.js",
    "start:dev": "nodemon",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,ts}\"",
    "lint": "eslint --cache \"**/*.{js,ts}\"",
    "dc:build": "docker-compose build --progress=plain --no-cache",
    "dc:up": "docker-compose up --build -d",
    "dc:down": "docker-compose down",
    "docker:build": "docker build -t bennodev19/dronies-watch-backend:latest .",
    "docker:push": "docker push bennodev19/dronies-watch-backend",
    "docker:release": "npm version patch && yarn run docker:build && yarn run docker:push"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/crypto-js": "^4.1.0",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/http-errors": "^1.8.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.24.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dotenv": "^10.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.17.2",
    "express-session": "^1.17.2",
    "helmet": "^5.0.1",
    "http-errors": "^2.0.0",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "rate-limiter-flexible": "^2.3.6",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.0",
    "typeorm": "^0.2.41"
  }
}
